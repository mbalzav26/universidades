
<%= simple_form_for [@university, @program] do |f| %>
  
  <div class="form-inputs">
    <%= f.input :nombre %>
    <%= f.input :beca %>
    <%= f.input :requisito %>
    <%= f.input :descripcion %>
    <%= f.association :university %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
